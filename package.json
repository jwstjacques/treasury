{
  "name": "treasury",
  "version": "1.0.0",
  "description": "Transaction tracking service.",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "dropdb": "rimraf database.sqlite3",
    "build": "npm run lint && tsc --project ./",
    "clean": "rimraf node_modules package-lock.json database.sqlite3",
    "lint": "eslint . --ext .ts",
    "lintfix": "eslint --fix . --ext .ts",
    "start": "tsc && nodemon dist/src/server.js",
    "seed": "node ./scripts/SeedDb.js",
    "test": "jest --coverage --colors  --runInBand --forceExit --detectOpenHandles"
  },
  "author": "Jason St. Jacques",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "dotenv": "^14.3.2",
    "express": "4.17.1",
    "jest-express": "^1.12.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "sequelize": "^6.16.1",
    "sqlite3": "^4.0.1",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.12",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "concurrently": "7.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.4.7",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.3.0",
    "node": "^16.13.0",
    "nodemon": "2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "4.5.5"
  }
}
